# AstraMind Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for Whisper transcription and GPT-4 summaries)
OPENAI_API_KEY=your_openai_api_key_here

# YouTube Data API v3 Key (required for video search and summaries)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Calendar API (optional, for reminder sync)
GOOGLE_CALENDAR_CREDENTIALS=path_to_google_credentials.json

# Backend API URL (frontend will use this)
VITE_API_URL=http://localhost:8000
